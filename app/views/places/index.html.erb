<div class="container">
  <div class="places_container">
    <% @places.each do |place| %>
      <div class="card-trip">
       <% place.photos.last(1).each do |photo| %>
        <%= cl_image_tag photo.key, crop: :fill %>
      <% end %>
        <div class="card-trip-infos">
          <div>
            <h2><%= place.city %></h2>
            <p><%= place.address %></p>
          </div>
          <h2 class="card-trip-pricing"><%= place.price %></h2>
        </div>
      </div>
    <% end %>
  </div>
</div>
